if [ -f ~/.dotfiles/bash/paths ]; then
  . ~/.dotfiles/bash/paths
fi

if [ -f ~/.dotfiles/bash/aliases ]; then
  . ~/.dotfiles/bash/aliases
fi

if [ -f ~/.dotfiles/bash/completions ]; then
  . ~/.dotfiles/bash/completions
fi

if [ -f ~/.dotfiles/bash/env ]; then
  . ~/.dotfiles/bash/env
fi

# RVM
[[ -s $HOME/.rvm/scripts/rvm ]] && source $HOME/.rvm/scripts/rvm

cd ~
