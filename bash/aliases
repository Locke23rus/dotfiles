# cd
alias ..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../..'

# Redcar Editor
alias redcar='redcar --fork'
alias redcar-dev='jruby ~/Applications/redcar/bin/redcar --fork'

# Ruby and Rails
alias r='rails'
alias a='autotest'
alias b='bundle'
alias rmigrate='rake db:migrate && rake db:test:prepare'

alias latest-npm='curl http://npmjs.org/install.sh | sh'
