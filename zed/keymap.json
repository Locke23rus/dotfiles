[
  {
    "context": "Workspace",
    "bindings": {
      "cmd-j": "workspace::OpenTerminal",
      "alt-j": "workspace::OpenTerminal",
      "alt-s": "workspace::Save",
      "alt-p": "file_finder::Toggle"
    }
  },
  {
    "context": "Pane && docked",
    "bindings": {
      "cmd-j": "workspace::ToggleBottomDock"
    }
  },
  {
    "context": "Pane",
    "bindings": {
      "ctrl-/": "pane::GoBack",
      "ctrl-?": "pane::GoForward"
    }
  },
  {
    "context": "Editor",
    "bindings": {
      "shift-f12": "editor::FindAllReferences",
      "alt-j": "workspace::ToggleBottomDock",
      "alt-/": "editor::ToggleComments"
    }
  },
  {
    "context": "Terminal",
    "bindings": {
      "alt-j": "workspace::ToggleBottomDock"
    }
  }
]
